{
  "name": "eslint-plugin-avbhub",
  "version": "0.1.0",
  "private": true,
  "description": "Custom ESLint rules for the HUB project.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "format": "prettier --write \"./**/*.{ts,mts,cts,tsx,js,mjs,cjs,jsx,json,md,css}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.46.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0"
  }
}
